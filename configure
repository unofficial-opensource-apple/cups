				CUPS_USER="$user"
				echo "$as_me:$LINENO: result: $user" >&5
echo "${ECHO_T}$user" >&6
				break;
			fi
		done

		if test x$CUPS_USER = x; then
			CUPS_USER="${USER:=nobody}"
			echo "$as_me:$LINENO: result: not found" >&5
